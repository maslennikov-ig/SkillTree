{
  "name": "@skilltree/database",
  "version": "0.2.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "ts-node --compiler-options '{\"module\":\"commonjs\"}' prisma/seed.ts",
    "db:check-retention": "ts-node --compiler-options '{\"module\":\"commonjs\"}' prisma/scripts/check-retention.ts",
    "db:anonymize": "ts-node --compiler-options '{\"module\":\"commonjs\"}' prisma/scripts/anonymize-users.ts",
    "type-check": "tsc --noEmit"
  },
  "prisma": {
    "seed": "ts-node --compiler-options '{\"module\":\"commonjs\"}' prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0"
  },
  "devDependencies": {
    "@skilltree/typescript-config": "workspace:*",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
